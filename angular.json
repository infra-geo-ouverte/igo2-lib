{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "demo": {
      "root": "",
      "sourceRoot": "demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "defaultConfiguration": "developpement",
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/demo",
            "index": "demo/src/index.html",
            "main": "demo/src/main.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.app.json",
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets",
              "demo/src/config",
              "demo/src/contexts",
              "demo/src/locale",
              {
                "glob": "**/*",
                "input": "./dist/core/assets/",
                "output": "./assets/igo2/core/"
              },
              {
                "glob": "**/*",
                "input": "./dist/geo/assets/",
                "output": "./assets/igo2/geo/"
              }
            ],
            "styles": [
              "demo/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "hammerjs",
              "typy",
              "jspdf",
              "file-saver",
              "jwt-decode",
              "canvg",
              "core-js-pure/features/array/from",
              "core-js-pure/features/array/is-array",
              "core-js-pure/features/instance/slice",
              "core-js-pure/features/object/create",
              "core-js-pure/features/object/define-property",
              "core-js-pure/features/object/get-own-property-descriptor",
              "core-js-pure/features/object/get-prototype-of",
              "core-js-pure/features/object/set-prototype-of",
              "core-js-pure/features/promise",
              "core-js-pure/features/reflect/get",
              "core-js-pure/features/symbol",
              "core-js-pure/features/symbol/iterator",
              "core-js/modules/es.array.index-of.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.array.reverse.js",
              "core-js/modules/es.promise.js",
              "core-js/modules/es.regexp.to-string.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "canvg/lib/index.es.js",
              "html-entities",
              "@turf/helpers",
              "@turf/line-segment",
              "@turf/meta",
              "@turf/invariant",
              "@turf/line-intersect",
              "pbf",
              "geojson-rbush",
              "minimatch",
              "path",
              "pixelworks",
              "brace-expansion",
              "raf",
              "pako/lib/inflate",
              "txml",
              "lru-cache",
              "striptags"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "developpement": {
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "7mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "demo/src/test.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/src/tsconfig.spec.json",
            "karmaConfig": "demo/src/karma.conf.js",
            "styles": [
              "demo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "demo/src/**/*.ts",
              "demo/src/**/*.html"
            ]
          }
        }
      }
    },
    "demo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "demo:serve"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "e2e/**/*.ts"
            ]
          }
        }
      }
    },
    "auth": {
      "root": "packages/auth",
      "sourceRoot": "packages/auth/src",
      "projectType": "library",
      "prefix": "igo",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "style": "scss"
        }
      },
      "architect": {
        "build": {
