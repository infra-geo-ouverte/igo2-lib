{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "packages",
  "projects": {
    "demo": {
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "defaultConfiguration": "developpement",
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets",
              "projects/demo/src/config",
              "projects/demo/src/contexts",
              "projects/demo/src/locale",
              {
                "glob": "**/*",
                "input": "./packages/common/src/assets/",
                "output": "./assets/igo2/common/"
              },
              {
                "glob": "**/*",
                "input": "./packages/geo/src/assets/",
                "output": "./assets/igo2/geo/"
              },
              {
                "glob": "**/*",
                "input": "./dist/core/assets/",
                "output": "./assets/igo2/core/"
              },
              {
                "glob": "**/*",
                "input": "./dist/core/locale/",
                "output": "./locale/libs_locale",
                "ignore": ["**/*.core.json"]
              },
              {
                "glob": "**/*",
                "input": "./dist/core/theming/prebuilt-themes",
                "output": "./assets/igo2/core/theming/prebuilt-themes"
              }
            ],
            "styles": ["projects/demo/src/styles.scss"],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "@turf",
              "bowser",
              "canvg",
              "core-js",
              "file-saver",
              "flexsearch",
              "html-entities",
              "html2canvas",
              "jspdf",
              "jspdf-autotable",
              "jszip",
              "moment",
              "nosleep.js",
              "raf",
              "rbush",
              "rgbcolor",
              "striptags",
              "ts-md5",
              "typy"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "7mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "github": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "outputPath": "dist/ghpages",
              "baseHref": "/igo2-lib/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "7mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "developpement": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "7mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "host": "127.0.0.1",
            "proxyConfig": "projects/demo/proxy.conf.json",
            "hmr": true,
            "buildTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "demo:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/demo/src/**/*.ts",
              "projects/demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "polyfills": ["projects/demo/src/polyfills.ts", "zone.js/testing"],
            "tsConfig": "projects/demo/tsconfig.spec.json",
            "karmaConfig": "projects/demo/karma.conf.js",
            "styles": ["projects/demo/src/styles.scss"],
            "scripts": [],
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets"
            ],
            "codeCoverage": true
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "configFile": "projects/demo/e2e/cypress.config.ts",
            "devServerTarget": "demo:serve",
            "browser": "chrome"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            },
            "local": {
              "watch": true,
              "headless": false
            }
          }
        }
      }
    },
    "auth": {
      "root": "packages/auth",
      "sourceRoot": "packages/auth/src",
      "projectType": "library",
      "prefix": "igo",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "packages/auth/tsconfig.lib.json",
            "project": "packages/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "packages/auth/ng-package.prod.json",
              "tsConfig": "packages/auth/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/auth/src/test.ts",
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "packages/auth/tsconfig.spec.json",
            "karmaConfig": "packages/auth/karma.conf.js",
            "codeCoverage": true
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "packages/auth/**/*.ts",
              "packages/auth/**/*.html"
            ]
          }
        }
      }
    },
