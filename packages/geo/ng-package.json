{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/geo",
  "lib": {
    "entryFile": "src/public_api.ts"
  },
  "allowedNonPeerDependencies": [
    "@mat-datetimepicker",
    "@turf",
    "file-saver",
    "flexsearch",
    "html2canvas",
    "jspdf-autotable",
    "jspdf",
    "jszip",
    "moment",
    "ngx-indexed-db",
    "nosleep.js",
    "striptags",
    "ts-cacheable",
    "ts-md5",
    "@turf/helpers",
    "@turf/boolean-intersects",
    "@turf/buffer",
    "@turf/intersect",
    "@turf/line-intersect",
    "@turf/point-on-feature"
  ],
  "assets": [
    { "input": "", "glob": "**/*.them*.scss", "output": "" },
    { "input": "src", "glob": "*.scss", "output": "src" },
    { "input": "src/style", "glob": "**/*.scss", "output": "src/style" },
    { "input": "src/assets", "glob": "**/*", "output": "assets" },
    { "input": "src/locale", "glob": "*", "output": "locale" }
  ]
}
