  },
  "bugs": {
    "url": "https://github.com/infra-geo-ouverte/igo2-lib/issues"
  },
  "scripts": {
    "build": "ng build integration --configuration production && npm run clean-exports",
    "clean-exports": "ts-node ../../scripts/src/core/clean-exports.ts integration",
