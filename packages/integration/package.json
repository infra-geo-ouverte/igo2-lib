    "@igo2/context": "*",
    "@igo2/geo": "*",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.29",
    "rxjs": "^7.8.0"
  },
  "engines": {
    "node": ">=18.10.0"
  }
