{
  "name": "@igo2/igo2",
  "version": "0.19.10",
  "description": "IGO Library",
  "main": "bundle/igo.js",
  "typings": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infra-geo-ouverte/igo2-lib.git"
  },
  "keywords": [
    "igo"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infra-geo-ouverte/igo2-lib/issues"
  },
  "homepage": "https://github.com/infra-geo-ouverte/igo2-lib#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --app demo --proxy-config proxy.conf.json",
    "start.lib": "ng serve --proxy-config proxy.conf.json",
    "build.lib": "npm run clean && gulp && ngc -p ./src/tsconfig.lib.json && npm run build.lib.bundle",
    "build.lib.bundle": "webpack --config webpack.conf.js  --bail --display-modules",
    "build.demo": "ng build --app demo",
    "build.demo.prod": "ng build --app demo --prod",
    "test": "npm run lint && ng test --single-run",
    "lint": "ng lint",
    "pree2e": "webdriver-manager clean && webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e --app demo",
    "clean": "rimraf dist && rimraf bundle",
    "language.extract": "npm run language.extract.lib && npm run language.extract.demo",
    "language.extract.lib": "ngx-translate-extract -i ./src/lib -o ./src/locale/fr.json ./src/locale/en.json -s -f namespaced-json -m _ -c",
    "language.extract.demo": "ngx-translate-extract -i ./src/demo-app -o ./src/demo-app/assets/locale/fr.json ./src/demo-app/assets/locale/en.json -s -f namespaced-json -m _ -c",
    "preversion": "npm test && npm run build.demo.prod && npm run build.lib",
    "postversion": "git push && git push --tags && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "^5.1.0",
    "@angular/cdk": "^5.1.0",
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/forms": "^5.1.0",
    "@angular/http": "^5.1.0",
    "@angular/material": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@auth0/angular-jwt": "^1.0.0-beta.9",
    "@mat-datetimepicker/core": "^1.0.4",
    "@ngx-translate/core": "^8.0.0",
    "angular2-notifications": "^1.0.1",
    "bowser": "^1.9.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jspdf": "^1.3.5",
    "jwt-decode": "^2.2.0",
    "ol": "^4.6.5",
    "rxjs": "^5.5.2",
    "ts-md5": "^1.2.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@angular/platform-server": "^5.1.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/geojson": "^7946.0.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jspdf": "^1.1.31",
    "@types/node": "^6.0.68",
    "@types/ol": "^4.6.2",
    "@types/openlayers": "^4.6.5",
    "@types/yargs": "^10.0.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "codelyzer": "^4.0.1",
    "css-loader": "^0.28.9",
    "gulp": "^3.9.1",
    "gulp-replace": "^0.6.1",
    "gulp-stylus": "^2.7.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "to-string-loader": "^1.1.5",
    "ts-node": "~3.2.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "author": "MSP"
}
