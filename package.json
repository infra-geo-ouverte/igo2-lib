    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lodash": "^4.17.21",
    "madge": "^6.1.0",
    "ng-packagr": "^16.2.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sass": "^1.64.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
