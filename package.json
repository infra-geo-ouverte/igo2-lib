{
  "name": "@igo/demo",
  "workspaces": [
    "./packages/*"
  ],
  "version": "1.15.4",
  "description": "IGO Library",
  "author": "IGO Community",
  "license": "MIT",
  "homepage": "https://github.com/infra-geo-ouverte/igo2-lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infra-geo-ouverte/igo2-lib.git"
  },
  "bugs": {
    "url": "https://github.com/infra-geo-ouverte/igo2-lib/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start.watch": "run-p watch.libs \"_start.watch -- -- {@}\" --",
    "build": "ng build",
    "lint": "ng lint",
    "lint.fix": "ng lint --fix",
    "e2e": "ng e2e",
    "build.all": "npm run build.libs && npm run build.demo",
    "build.libs": "rimraf ./dist && npm run build.utils && npm run build.core && npm run build.common && npm run build.auth && npm run build.geo && npm run build.context && npm run build.integration && npm run build.locale && gulp bumpVersion",
    "build.utils": "ng build utils --configuration production",
    "build.core": "ng build core --configuration production",
    "build.common": "ng build common --configuration production",
    "build.auth": "ng build auth --configuration production",
    "build.geo": "ng build geo --configuration production",
    "build.context": "ng build context --configuration production",
    "build.integration": "ng build integration --configuration production",
    "build.demo": "ng build demo --configuration=production",
    "build.ghpages": "ng build demo --configuration=production --output-path ./dist/ghpages --base-href /igo2-lib/",
    "build.doc": "compodoc -p tsconfig.json --disablePrivate --disableProtected --disableInternal --disableLifeCycleHooks --theme material",
    "copy": "copy-newer --cwd dist ** \"..\\igo2\\node_modules\\@igo2\"",
    "doc": "npm run build.doc && compodoc -s --port 4220",
    "pretest": "ng lint",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test.watch": "ng test",
    "test.all": "npm run test.libs && npm run test.demo",
    "test.libs": "npm run lint && npm run test.core && npm run test.common && npm run test.geo && npm run test.context",
    "test.core": "ng test core --watch=false --code-coverage",
    "test.common": "ng test common --watch=false --code-coverage",
    "test.geo": "ng test geo --watch=false --code-coverage",
    "test.context": "ng test context --watch=false --code-coverage",
    "test.demo": "ng test demo --watch=false --code-coverage && ng e2e --port 4300",
    "watch.libs": "rimraf ./dist && run-p _watch.*",
    "_watch.utils": "ng build utils --watch",
    "_watch.core": "wait-on dist/utils/package.json && ng build core --watch",
    "_watch.common": "wait-on dist/core/package.json && ng build common --watch",
    "_watch.auth": "wait-on dist/common/package.json && ng build auth --watch",
    "_watch.geo": "wait-on dist/auth/package.json && ng build geo --watch",
    "_watch.context": "wait-on dist/geo/package.json && ng build context --watch",
    "_watch.integration": "wait-on dist/context/package.json && ng build integration --watch",
    "_start.watch": "wait-on dist/core/locale/fr.json && npm start",
    "gulp": "gulp",
    "clean": "rimraf ./dist && rimraf ./node_modules && npm cache clean --force && git checkout -- package.json && git checkout -- package-lock.json",
    "compodoc": "npx compodoc -p tsconfig.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog-current": "conventional-changelog -p angular -r 2",
    "publish": "gulp prepublishOnly && npm publish ./dist/utils && npm publish ./dist/core && npm publish ./dist/common && npm publish ./dist/auth && npm publish ./dist/geo && npm publish ./dist/context && npm publish ./dist/integration",
    "preversion": "npm run test.all",
    "preversion.linux": "npm run test.all",
    "version": "gulp bumpVersion && rimraf ./dist && npm run build.all && npm run changelog && git add -u",
    "version.linux": "gulp bumpVersion && rimraf ./dist && npm run build.all && npm run changelog && git add -u",
    "postversion": "git push && git push --tags && npm run publish && npm run ghpages.win",
    "postversion.linux": "git push && git push --tags && npm run publish && npm run ghpages",
    "preghpages": "gulp bundleLocale && npm run build.ghpages && echo \"include: ['_default.json', '_contexts.json', '_base.json']\" > dist/ghpages/_config.yml",
    "ghpages": "VERSION=$(node -p -e \"require('./package.json').version\") && npx ngh --dir=dist/ghpages --no-silent=false --message=$VERSION",
    "postghpages": "rimraf ./dist/ghpages",
    "preghpages.win": "gulp bundleLocale && npm run build.ghpages && echo include: ['_default.json', '_contexts.json', '_base.json'] > dist/ghpages/_config.yml",
    "ghpages.win": "npx ngh --dir=dist/ghpages --no-silent=false --message=%npm_package_version%",
    "postghpages.win": "rimraf ./dist/ghpages",
    "postbuild.core": "ts-node scripts/src/postbuild-core.ts",
    "postbuild.common": "ts-node scripts/src/postbuild-common.ts",
    "postbuild.geo": "ts-node scripts/src/postbuild-geo.ts",
    "build.locale": "ts-node scripts/src/locale-merge.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.9",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.9",
    "@angular/compiler": "^15.2.9",
    "@angular/core": "^15.2.9",
    "@angular/forms": "^15.2.9",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.9",
    "@angular/platform-browser-dynamic": "^15.2.9",
    "@angular/router": "^15.2.9",
    "@azure/msal-angular": "^2.4.1",
    "@azure/msal-browser": "^2.28.1",
    "@mat-datetimepicker/core": "~11.0.0",
    "@mdi/angular-material": "^7.2.96",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@turf/buffer": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/line-intersect": "^6.5.0",
    "@turf/point-on-feature": "^6.5.0",
    "angular-shepherd": "15.0.0",
    "bowser": "^2.10.0",
    "core-js": "^3.30.2",
    "file-saver": "^2.0.2",
    "flexsearch": "0.7.21",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.29",
    "jszip": "^3.10.1",
    "jwt-decode": "^2.2.0",
    "moment": "^2.29.4",
    "ngx-color-picker": "^13.0.0",
    "ngx-indexed-db": "^11.0.2",
    "ngx-toastr": "^15.0.0",
    "nosleep.js": "^0.12.0",
    "ol": "^7.3.0",
    "proj4": "^2.8.0",
    "rxjs": "^7.5.6",
    "scroll-into-view-if-needed": "^2.2.29",
    "striptags": "^3.2.0",
    "ts-cacheable": "^1.0.6",
    "ts-md5": "^1.2.4",
    "tslib": "^2.4.0",
    "typy": "^3.3.0",
    "windows-1252": "^3.0.4",
    "zone.js": "~0.11.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.9",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.9",
    "@angular/compiler-cli": "^15.2.9",
    "@compodoc/compodoc": "^1.1.15",
    "@types/flexsearch": "^0.7.3",
    "@types/geojson": "^7946.0.10",
    "@types/html2canvas": "0.0.35",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.195",
    "@types/node": "^14.18.23",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.33.0",
    "angular-cli-ghpages": "^1.0.3",
    "conventional-changelog-cli": "^2.0.12",
    "copy-newer": "^2.1.2",
    "eslint": "^8.21.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-json-editor": "^2.4.2",
    "gulp-replace": "^1.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lodash": "^4.17.21",
    "ng-packagr": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "sass": "^1.54.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "wait-on": "^6.0.0"
  },
  "engines": {
    "node": ">=16.19.0"
  }
}