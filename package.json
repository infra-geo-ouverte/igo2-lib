{
  "name": "@igo/demo",
  "version": "0.22.0",
  "description": "IGO Library",
  "author": "MSP",
  "license": "MIT",
  "homepage": "https://github.com/infra-geo-ouverte/igo2-lib#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infra-geo-ouverte/igo2-lib.git"
  },
  "bugs": {
    "url": "https://github.com/infra-geo-ouverte/igo2-lib/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "lint": "ng lint",
    "test": "ng test",
    "e2e": "ng e2e",
    "build.libs": "npm run build.utils && npm run build.core && npm run build.common && npm run build.auth && npm run build.geo && npm run build.geoContext && npm run build.tools",
    "build.utils": "ng build utils --prod",
    "build.core": "ng build core --prod && gulp core",
    "build.common": "ng build common --prod && gulp common",
    "build.auth": "ng build auth --prod && gulp auth",
    "build.geo": "ng build geo --prod && gulp geo",
    "build.geoContext": "ng build geoContext --prod && gulp geoContext",
    "build.tools": "ng build tools --prod && gulp tools",
    "test.libs": "npm run lint && npm run test.core && npm run test.common && npm run test.geo && npm run test.geoContext && npm run test.demo",
    "test.core": "ng test core --watch=false --code-coverage",
    "test.common": "ng test common --watch=false --code-coverage",
    "test.geo": "ng test geo --watch=false --code-coverage",
    "test.geoContext": "ng test geoContext --watch=false --code-coverage",
    "test.demo": "ng test demo --watch=false --code-coverage && ng e2e --port 4300",
    "publish": "npm publish ./dist/utils && npm publish ./dist/core && npm publish ./dist/common && npm publish ./dist/auth && npm publish ./dist/geo && npm publish ./dist/geo-context && npm publish ./dist/tools",
    "preversion": "npm run test.libs && npm run build.libs",
    "version": "gulp bumpVersion && git add .",
    "postversion": "gulp bumpVersion && git push && git push --tags && npm run publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@mat-datetimepicker/core": "^2.0.1",
    "@ngx-translate/core": "^10.0.1",
    "angular2-notifications": "^1.0.2",
    "bowser": "^1.9.2",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ol": "^5.1.3",
    "proj4": "^2.4.4",
    "rxjs": "^6.1.0",
    "ts-md5": "^1.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular-devkit/build-ng-packagr": "~0.6.1",
    "@angular/cli": "~6.0.1",
    "@angular/compiler-cli": "^6.0.0",
    "@types/geojson": "^7946.0.3",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-exec": "^3.0.2",
    "gulp-json-editor": "^2.4.2",
    "gulp-merge-json": "^1.3.1",
    "gulp-sequence": "^1.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "scss-bundle": "^2.3.2",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
